{
  "name": "darma-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Darma Frontend - Cross-chain credit dashboard with AI agents and real protocol integration",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:prod": "tsc && vite build --mode production",
    "build:analyze": "tsc && vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "serve": "vite preview --port 3000 --host",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:ipfs": "npm run build && npx ipfs-deploy dist",
    "generate:types": "wagmi generate",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "ethers": "^6.8.0",
    "wagmi": "^2.0.0",
    "viem": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "axios": "^1.6.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.288.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "framer-motion": "^10.16.0",
    "react-hook-form": "^7.47.0",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1",
    "web3": "^4.2.0",
    "qrcode.react": "^3.1.0",
    "copy-to-clipboard": "^3.3.3",
    "react-tooltip": "^5.21.0",
    "react-helmet-async": "^2.0.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "debounce": "^1.2.1",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/js-cookie": "^3.0.6",
    "@types/debounce": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "@vitest/ui": "^0.34.6",
    "vitest": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6",
    "jsdom": "^22.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "playwright": "^1.40.0",
    "vite-bundle-analyzer": "^1.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darma-protocol/frontend.git"
  },
  "keywords": [
    "defi",
    "credit-scoring",
    "blockchain",
    "react",
    "typescript",
    "ethereum",
    "lending",
    "aave",
    "morpho",
    "pyth",
    "cross-chain",
    "ai-agents"
  ],
  "author": "Darma Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darma-protocol/frontend/issues"
  },
  "homepage": "https://darma.io"
}